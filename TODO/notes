  ./bin/validate -x 'data:text/shex,PREFIX : <l://> PREFIX foaf: <foaf://> start={ $:gn foaf:givenName .+; $:fn foaf:familyName .+ }' -d 'data:text/turtle,PREFIX : <d://> PREFIX foaf: <foaf://> :n1 foaf:givenName "c","d"; foaf:familyName "a".'
